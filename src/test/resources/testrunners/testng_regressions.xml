<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Test Regression PW Suite" thread-count="3"
	parallel="methods" verbose="4">

	<listeners>
		<listener
			class-name="com.hm.culturekings.listeners.ExtentReportListener" />

	</listeners>

	<test name="CultureKing">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.hm.culturekings.tests.ShippingPageTest" />

			<class name="com.hm.culturekings.tests.HomePageTest" />
		</classes>

	</test>



</suite>